import app
#import interface.dashboard_aluno as interface_dashboard_aluno
import interface.student as interface_student
#import connection.student as connection_student
import PySimpleGUI as sg

def create_window():
    layout = [
                [sg.Text('Em desenvolvimento...')],
                [sg.Button('Voltar', key='return', s=(18, 1))]
            ]

    return sg.Window('Avaliação 360 - Aluno', layout, element_justification='c')

def event_handler(event, _):
    if event == 'return':
        app.change_interface(interface_student.create_window(), interface_student.event_handler)
